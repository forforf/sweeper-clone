{
  "compilerOptions": {
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ES2022",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@app/*": ["app/*"],
      "@components/*": ["app/components/*"],
      "@pages/*": ["app/pages/*"],
      "@routes/*": ["app/routes/*"]
    }
  },
}